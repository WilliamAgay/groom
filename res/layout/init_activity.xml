<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/RelativeLayoutInitActivityContainer"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:layout_margin="0dip"
    android:background="@android:color/transparent"
    android:orientation="vertical"
    android:padding="0dip" >

    <LinearLayout
        android:id="@+id/LinearLayoutInitActivityContainerHeader"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal" >

        <RelativeLayout
            android:id="@+id/RelativeLayoutInitActivityHeaderContainer"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_margin="0dip"
            android:background="#000000"
            android:padding="5dip" >

            <TextView
                android:id="@+id/TextViewInitActivityHeader"
                style="@style/styleSettingsTxt"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_centerVertical="true"
                android:paddingLeft="8dip"
                android:text="Bienvenue !" />

            <Button
                android:id="@+id/ButtonInitActivityGo"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="10dip"
                android:background="@drawable/ic_arrow_white" />
        </RelativeLayout>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/LinearLayoutInitActivityContainerTiles"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentRight="true"
        android:layout_below="@+id/LinearLayoutInitActivityContainerHeader"
        android:layout_above="@+id/LinearLayoutInitActivityContainerFooter"
        android:layout_margin="0dip"
        android:background="#000000"
        android:orientation="horizontal"
        android:padding="0dip" >

        <RelativeLayout
            android:id="@+id/RelativeLayoutInitActivityLine1Tiles"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:layout_weight="1"
            android:layout_margin="0dip"
            android:background="#000000"
            android:padding="0dip" >
            
       </RelativeLayout>
       
        <View
              android:id="@+id/ViewInitActivityLine1TilesSeparator"
             android:layout_width="fill_parent"
                android:layout_height="1dip"
                android:background="#000000" />
        
         <RelativeLayout
            android:id="@+id/RelativeLayoutInitActivityLine2Tiles"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:layout_weight="1"
            android:layout_margin="0dip"
            android:background="#000000"
            android:padding="0dip" >
            
       </RelativeLayout>
       
        <View
              android:id="@+id/ViewInitActivityLine2TilesSeparator"
             android:layout_width="fill_parent"
                android:layout_height="1dip"
                android:background="#000000" />
        
         <RelativeLayout
            android:id="@+id/RelativeLayoutInitActivityLine3Tiles"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:layout_weight="1"
            android:layout_margin="0dip"
            android:background="#000000"
            android:padding="0dip" >
            
       </RelativeLayout>
       
        <View
              android:id="@+id/ViewInitActivityLine3TilesSeparator"
             android:layout_width="fill_parent"
                android:layout_height="1dip"
                android:background="#000000" />

        </LinearLayout>
